{
    "name": "mirko-pagliai/cakephp-thumber",
    "description": "Thumber plugin for CakePHP",
    "homepage": "https://github.com/mirko-pagliai/cakephp-thumber",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [{
            "name": "Mirko Pagliai",
            "email": "mirko.pagliai@gmail.com"
        }],
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "4.x-dev",
        "intervention/image": "^2.4",
        "mirko-pagliai/me-tools": "dev-cakephp4",
        "mirko-pagliai/php-thumber": "dev-php7",
        "mirko-pagliai/php-tools": "dev-php7"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "dev-next",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Thumber\\Cake\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/test_app/TestApp/",
            "Thumber\\Cake\\Test\\": "tests"
        }
    }
}
